<Window x:Class="FE_Analysis.Heat_Transfer.ModelDataRead.BoundaryCondionsKeys"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="existing BoundaryCondition definitions" Height="200" Width="300">
    <Grid>
        <ListView Margin="10" Name="RandbedingungKey">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="ID" Width="30" DisplayMemberBinding="{Binding SupportId}" />
                    <GridViewColumn Header="Node" Width="100" DisplayMemberBinding="{Binding NodeId}" />
                    <GridViewColumn Header="Element" Width="100" DisplayMemberBinding="{Binding ElementId}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>